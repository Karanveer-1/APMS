<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="apms">
		<jta-data-source>java:jboss/datasources/apms</jta-data-source>
		<class>Model.Employee</class>
		<class>Model.ProAssi</class>
<!-- 		<class>Model.ProEmp</class> -->
		<!-- <class>Model.Project</class> <class>Model.ProjectForm</class> <class>Model.WorkPackage</class> 
			<class>Model.WorkPackageForm</class> <class>Model.WorkPackageRelationship</class> -->
		<!-- <class>Model.WPEmp</class> -->




		<properties>
			<property name="hibernate.dialect"
				value="org.hibernate.dialect.MySQLInnoDBDialect" />
			<property name="hibernate.hbm2ddl.auto" value="validate" />
			<property name="hibernate.show_sql" value="false" />
			<property name="hibernate.format_sql" value="true" />
		</properties>
	</persistence-unit>
</persistence>
